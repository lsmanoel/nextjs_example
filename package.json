{
  "name": "curriculum",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "lint": "next lint",
    "test": "jest --passWithNoTests --runInBand --no-cache",
    "test:fetch-mock": "npm test -- --watch -t 'fetch-mock test'",
    "test:watch:utils": "npm test -- 'utils' --watch",
    "test:watch": "npm test -- --watch",
    "test:watch-silent": "npm test -- --watch --silent",
    "test:staged": "npm test -- findRelatedTests",
    "test:ci": "npm test -- --coverage",
    "test:coveralls": "npm run test:ci && coveralls < coverage/lcov.info ",
    "cy": "cypress open",
    "cy:test:chrome": "cypress run --browser chrome",
    "cy:test:firefox": "cypress run --browser firefox",
    "cy:test:chromium": "cypress run --browser chromium",
    "prepare": "husky install"
  },
  "prettier": {},
  "dependencies": {
    "@emailjs/browser": "^3.6.2",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-brands-svg-icons": "^6.4.0",
    "@fortawesome/free-regular-svg-icons": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@tippyjs/react": "^4.2.6",
    "date-fns": "^2.30.0",
    "firebase": "^9.22.2",
    "firebase-admin": "^11.9.0",
    "next": "^12.2.0",
    "next-auth": "^4.22.1",
    "react": "18.2.0",
    "react-build-status-badge": "^1.0.8",
    "react-code-blocks": "^0.0.9-0",
    "react-dom": "18.2.0",
    "sass": "^1.52.3"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^5.12.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.1",
    "@types/jest": "^28.1.4",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.14",
    "@types/react-dom": "18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "cypress": "^10.3.0",
    "eslint": "^8.17.0",
    "eslint-config-next": "^12.2.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.2",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.0",
    "husky": "^8.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest-environment-jsdom": "^28.1.2",
    "jest-fetch-mock": "^3.0.3",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.0",
    "ts-jest": "^28.0.5",
    "ts-loader": "^9.3.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4"
  }
}
